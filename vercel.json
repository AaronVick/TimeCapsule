{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/triviaFrame",
      "dest": "/api/triviaFrame",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/og",
      "dest": "/api/og",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/historyFrame",
      "dest": "/api/historyFrame",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}